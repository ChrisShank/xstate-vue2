{
  "name": "xstate-vue2",
  "version": "0.3.0",
  "description": "XState tools for Vue",
  "keywords": [
    "state",
    "machine",
    "statechart",
    "scxml",
    "state",
    "graph",
    "vue",
    "composable"
  ],
  "author": "Christopher Shank <chris.shank.23@gmail.com>",
  "homepage": "https://github.com/ChrisShank/xstate-vue2#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "dist/**/*.js",
    "es/**/*.js",
    "es/**/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ChrisShank/xstate-vue2.git"
  },
  "scripts": {
    "clean": "rm -rf dist lib es tsconfig.tsbuildinfo",
    "build": "tsc && tsc --outDir es --module es2015 && rollup -c",
    "test": "jest"
  },
  "bugs": {
    "url": "https://github.com/ChrisShank/xstate-vue2/issues"
  },
  "peerDependencies": {
    "@xstate/fsm": "^1.0.0",
    "vue": "^2.7.0",
    "xstate": "^4.20.0"
  },
  "peerDependenciesMeta": {
    "@xstate/fsm": {
      "optional": true
    },
    "xstate": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/vue": "^5.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "@xstate/fsm": "^1.0.0",
    "babel-core": "^6.26.3",
    "husky": "^3.0.0",
    "jest": "^26.4.2",
    "lint-staged": "^8.0.0",
    "prettier": "^2.3.0",
    "rollup": "^2.48.0",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.4.0",
    "typescript": "^4.3.2",
    "vue": "^2.7.0",
    "vue-jest": "^3.0.0",
    "vue-template-compiler": "^2.7.0",
    "xstate": "^4.20.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
